                    "appcache-manifest": ["error", {"minScore": 1}],
                    "is-on-https": ["error", {"minScore": 1}],
                    "uses-passive-event-listeners": ["error", {"minScore": 1}],
                    "no-document-write": ["error", {"minScore": 1}],
                    "external-anchors-use-rel-noopener": ["error", {"minScore": 1}],
