# .coveragerc to control coverage.py
[run]
omit =
    */oppia_tools/*
    */third_party/*
    */usr/*
    *_test.py
    *core/tests/*
    *__init__.py
    # TODO(lilithxxx): Remove python_utils from the list once the codebase is
    # run under Python 3. See issue: https://github.com/oppia/oppia/issues/7419.
    python_utils.py
