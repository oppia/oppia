{
  "extends": [
    "eslint:recommended",
    "angular"
  ],
  "plugins": [
    "html"
  ],
  "env": {
    "browser": true,
    "jasmine": true,
    "jquery": true
  },
  "rules": {
    "no-empty": [
      "error",
      {
        "allowEmptyCatch": true
      }
    ],
    "func-style": [
      "error",
      "expression"
    ],
    "no-multi-str": [
      "error"
    ],
    "no-multi-spaces": [
      "error"
    ],
    "one-var": [
      "error",
      {
        "uninitialized": "always",
        "initialized": "never"
      }
    ],
    "padded-blocks": [
      "error",
      "never"
    ],
    "quote-props": [
      "error",
      "as-needed"
    ],
    "space-unary-ops": [
      "error",
      {
        "words": true,
        "nonwords": false,
      }
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "indent": [
      "error",
      2
    ],
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ],
    "curly": [
      "error",
      "all"
    ],
    "dot-notation": [
      "error",
      {
        "allowKeywords": true
      }
    ],
    "operator-linebreak": [
      "error",
      "after"
    ],
    "wrap-iife": [
      "error",
      "any"
    ],
    "space-infix-ops": [
      "error"
    ],
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ]
  }
}
