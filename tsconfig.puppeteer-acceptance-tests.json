{
  "compilerOptions": {
    "target": "ES2017",
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "rootDir": "core/tests/puppeteer-acceptance-tests",
    "outDir": "core/tests/puppeteer-acceptance-tests/build",
    "typeRoots": [
      "node_modules/@types",
      "core/tests"
    ],
    "types": [
      "node",
      "jasmine",
      "puppeteer",
      "puppeteer-acceptance-tests"
    ],
  },
  "include": [
    "core/tests/puppeteer-acceptance-tests/puppeteer-testing-utilities",
    "core/tests/puppeteer-acceptance-tests/spec",
    "core/tests/puppeteer-acceptance-tests/user-utilities",
    "core/tests/puppeteer-acceptance-tests/helpers"
  ],
}